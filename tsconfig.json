{
  "compilerOptions": {
    "target": "es5",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "sourceMap": true,
  },
  "include": ["src/**/*", "src/custom.d.ts"],
  "exclude": ["node_modules"]
}
